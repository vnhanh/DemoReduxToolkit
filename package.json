{
  "name": "DemoReduxToolkit",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "clean-android": "cd android && rm -Rf build && rm -Rf app/build && cd ../",
    "ios": "react-native run-ios",
    "build-ios": "yarn clean-ios && cd ios && pod install && cd ../",
    "clean-ios": "cd ios && rm -Rf Pods && rm -Rf Podfile.lock && rm -Rf build && cd ../",
    "refresh": "yarn clean-ios && yarn clean-android && rm -Rf yarn.lock && rm -Rf node_modules && yarn",
    "refresh-ios": "yarn clean-ios && yarn clean-android && rm -Rf yarn.lock && rm -Rf node_modules && yarn && yarn build-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-community/checkbox": "^0.5.15",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.5",
    "react": "18.2.0",
    "react-native": "0.71.5",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.5"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.5",
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "2",
    "eslint-plugin-node": "11",
    "eslint-plugin-promise": "4",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.8.7",
    "react-test-renderer": "18.2.0",
    "redux-logger": "^3.0.6",
    "typescript": "^5.0.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
